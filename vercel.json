{
  "buildCommand": "npm ci && npm run build",
  "outputDirectory": "build",
  "installCommand": "npm ci",
  "framework": "vite",
  "functions": {
    "app/api/**/*.js": {
      "runtime": "nodejs22.x"
    }
  },
  "routes": [
    {
      "src": "/static/(.*)",
      "headers": {
        "cache-control": "s-maxage=31536000,immutable"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
